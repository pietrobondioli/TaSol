{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "#{AllowedHosts}#",
  "ConnectionStrings": {
    "Database": "#{ConnectionStrings.Database}#"
  },
  "ApiSettings": {
    "ApiUrl": "#{ApiSettings.ApiUrl}#",
    "ApiVersion": "#{ApiSettings.ApiVersion}#",
    "ApiKey": "#{ApiSettings.ApiKey}#"
  },
  "JwtSettings": {
    "Secret": "#{JwtSettings.Secret}#",
    "Issuer": "#{JwtSettings.Issuer}#",
    "Audience": "#{JwtSettings.Audience}#",
    "ExpirationInMinutes": "#{JwtSettings.AccessTokenExpiration}#"
  },
  "AppSettings": {
    "ApplicationName": "TaSol",
    "BaseUrl": "#{AppSettings.BaseUrl}#",
    "ConfirmAccountUrl": "#{AppSettings.ConfirmAccountUrl}#",
    "ChangePasswordUrl": "#{AppSettings.ChangePasswordUrl}#",
    "ChangeEmailUrl": "#{AppSettings.ChangeEmailUrl}#",
    "LockAccountUrl": "#{AppSettings.LockAccountUrl}#"
  },
  "MqttSettings": {
    "BrokerAddress": "#{MqttSettings.BrokerAddress}#",
    "BrokerPort": "#{MqttSettings.BrokerPort}#",
    "Username": "#{MqttSettings.Username}#",
    "Password": "#{MqttSettings.Password}#",
    "ClientId": "#{MqttSettings.ClientId}#",
    "Topic": "#{MqttSettings.Topic}#"
  }
}
